program sumaPrimerosDiez
    [*
        Este programa calcula la suma de los primeros 10 n√∫meros enteros, declarando la variable
        del for fuera. *]
        
    int acumulador;
    int i;
    {
    acumulador = 0;
    i = 1;
    for (i <= 10; i = i + 1) { # Se declara la variable que recorre el bucle fuera -> ERROR
        acumulador = acumulador + i;
    };
    println(acumulador);
}