program sumaPrimerosDiez
    int acumulador, entrada;
{
    acumulador = 0;
    read(entrada);
    
    for (int i = 1; i <= 10; i = i + 1) {
        acumulador = acumulador + i;
        
        [* Sale del for si acumulador < 0 y la entrada es > 0 *]
        if (acumulador < 0 and entrada > 0) {
            exit;
        };
    };
    println(acumulador);
} 